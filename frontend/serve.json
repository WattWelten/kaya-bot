{
  "public": "dist",
  "headers": [
    {
      "source": "**/*.{html,htm}",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "source": "**/*.{js,css,png,jpg,jpeg,gif,svg,webp,ico,json,webmanifest,woff,woff2,eot,ttf,otf,glb}",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ]
}

